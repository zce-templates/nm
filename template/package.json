{
  "name": "{{name}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "keywords": [
    {{#cli}}
    "cli",
    {{/cli}}
    "zce"
  ],
  "homepage": "https://github.com/{{user}}/{{name}}#readme",
  "bugs": {
    "url": "https://github.com/{{user}}/{{name}}/issues"
  },
  "license": "{{license}}",
  "author": "{{author}}",
  "files": [
    {{#cli}}
    "bin",
    {{/cli}}
    "lib"
  ],
  "main": "lib/index.js",
  {{#cli}}
  "bin": {
    "{{name}}": "bin/{{name}}.js"
  },
  {{/cli}}
  "directories": {
    "lib": "lib"{{#doc}},
    "doc": "doc"{{/doc}}{{#example}},
    "example": "example"{{/example}}{{#test}},
    "test": "test"{{/test}}
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/{{user}}/{{name}}.git"
  },
  "scripts": {
    "lint": "standard --fix"{{#test}},
    "test": "npm run lint &&{{#coverage}} nyc{{/coverage}} ava"{{/test}}
  },
  "dependencies": {
    {{#cli}}
    "commander": "^2.11.0"
    {{/cli}}
  },
  "devDependencies": {
    {{#test}}
    "ava": "^0.23.0",
    {{/test}}
    {{#coverage}}
    "nyc": "^11.3.0",
    {{/coverage}}
    "standard": "^10.0.3"
  },
  "engines": {
    "node": ">=6"
  }{{#cli}},
  "preferGlobal": true{{/cli}}
}
